import java.util.HashMap;
import java.util.Map;

class NurseLogin {
    private int id;
    private String fullName;
    private String username;
    private String password;

    public Nurse(int id, String fullName, String username, String password) {
        this.id = id;
        this.fullName = fullName;
        this.username = username;
        this.password = password;
    }

    public int getId() {
        return id;
    }

    public String getFullName() {
        return fullName;
    }

    public String getUsername() {
        return username;
    }

    public String getPassword() {
        return password;
    }
}

public class NurseAccountManager {
    public static void main(String[] args) {
        
        Map<Integer, Nurse> nurseMap = new HashMap<>();

        
        Nurse nurse1 = new Nurse(1, "Nurse Anne", "anne@sample.com", "nursePass123");
        Nurse nurse2 = new Nurse(2, "Nurse Robert", "robert@sample.com", "nurseSecurePass");

        nurseMap.put(nurse1.getId(), nurse1);
        nurseMap.put(nurse2.getId(), nurse2);

        
        int nurseId = 1;
        Nurse retrievedNurse = nurseMap.get(nurseId);
        System.out.println("Nurse ID: " + retrievedNurse.getId() +
                ", Full Name: " + retrievedNurse.getFullName() +
                ", Username: " + retrievedNurse.getUsername() +
                ", Password: " + retrievedNurse.getPassword());
    }
}
