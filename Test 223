import java.util.HashMap;
import java.util.LinkedList;
import java.util.Map;

public class HashMapWithLinkedList {
    public static void main(String[] args) {
        // Create a HashMap with Integer keys and LinkedList<String> values
        Map<Integer, LinkedList<String>> hashMap = new HashMap<>();

        // Create a new LinkedList
        LinkedList<String> list = new LinkedList<>();

        // Add elements to the LinkedList
        list.add("Element 1");
        list.add("Element 2");
        list.add("Element 3");

        // Put the LinkedList into the HashMap with a specific key
        hashMap.put(1, list);

        // Retrieve the LinkedList from the HashMap using the key
        LinkedList<String> retrievedList = hashMap.get(1);

        // Display the elements of the retrieved LinkedList
        if (retrievedList != null) {
            for (String element : retrievedList) {
                System.out.println(element);
            }
        }
    }
}
